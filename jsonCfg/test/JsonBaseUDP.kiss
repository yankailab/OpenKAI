	}
	"vBASE":["udpClient","udpServer","JsonSender","JsonReceiver","TestJSON"],
}
"udpClient":{
	"class":"_UDP",
	"bON": true,
"thread":{
		"FPS":30,
	}
	"nPacket":10,
	"nPbuffer":512,
	"addr":"127.0.0.1",
	"port":19840,
	"bServer":0,
	"bW2R":1,
	"bWbroadcast":0,
}
"udpServer":{
	"class":"_UDP",
	"bON": true,
"thread":{
		"FPS":30,
	}
	"nPacket":10,
	"nPbuffer":512,
	"port":19840,
	"bServer":1,
	"bW2R":1,
	"bWbroadcast":0,
}
"JsonSender":{
	"class":"_JSONbase",
	"bON": true,
	"bLog":false,
"thread":{
		"FPS":1,
	}
"threadR":{
		"FPS":1,
	}
	"msgFinishSend":"EOJ",
	"msgFinishRecv":"EOJ",
	"_IObase":"udpClient",
}
"JsonReceiver":{
	"class":"_JSONbase",
	"bON": true,
	"bLog":false,
