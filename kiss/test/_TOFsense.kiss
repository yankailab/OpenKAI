{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":0,
	"bStdErr":1,
}

{
    "name":"console",
    "class":"_Console",
    "bON":1,
    {
        "name":"thread",
        "FPS":30,
    }
    "vBASE":["serial","TOFsense"],
}

{
	"name":"serial",
	"class":"_SerialPort",
	"bON":1,
    {
        "name":"thread",
        "FPS":30,
    }
    {
        "name":"threadR",
        "FPS":30,
    }
	"bLog":1,
	"port":"/dev/ttyAMA1",
	"baud":921600,
}

{
	"name":"TOFsense",
	"class":"_TOFsense",
	"bON":1,
    {
        "name":"thread",
        "FPS":30,
    }
	"bLog":1,
	"_IOBase":"serial",
}