{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":0,
	"bStdErr":1,
}

{
	"name":"console",
	"class":"_Console",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"vBASE":["modbus","stepper"],
}

{
	"name":"modbus",
	"class":"_Modbus",
	"bON":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"bLog":0,
	"port":"/dev/ttyUSB0",
	"/baud":9600,
	"baud":115200,
	"parity":"N",	
}

{
	"name":"stepperX",
	"class":"_HYMCU_RS485",
	"bON":1,
    {
		"name":"thread",
		"FPS":5,
	}
	"bLog":1,
	"_Modbus":"modbus",
	"iSlave":2,
	"dpr":1,
	"bFeedback":1,
	"dInit":5,

	{
		"name":"addr",
		"setDPR":4,
		"setDist":9,
		"setDir":11,
		"setSpd":8,
		"setAcc":2,
		"bComplete":12,
		"readStat":22,
		"run":7,
		"stop":3,
		"resPos":10,
	}

	{
		"name":"axis",
		{
			"name":"X",
			"pOrigin":0.0,
			"pTarget":5.0,
			"pErr":0.01,
			"pRange":[0,15],
			"sTarget":10.0,
			"sRange":[0,300],
			"aTarget":1.0,
			"aRange":[1,300],
		}
	}
}

{
	"name":"stepperY",
	"class":"_HYMCU_RS485",
	"bON":0,
    {
		"name":"thread",
		"FPS":5,
	}
	"bLog":1,
	"_Modbus":"modbus",
	"iSlave":2,
	"dpr":1,
	"bFeedback":1,
	"dInit":80,

	{
		"name":"addr",
		"setDPR":4,
		"setDist":9,
		"setDir":11,
		"setSpd":8,
		"setAcc":2,
		"bComplete":12,
		"readStat":22,
		"run":7,
		"stop":3,
		"resPos":10,
	}

	{
		"name":"axis",
		{
			"name":"Y",
			"pOrigin":0.0,
			"pTarget":80.0,
			"pErr":0.01,
			"pRange":[0,15],
			"sTarget":300.0,
			"sRange":[0,300],
			"aTarget":10.0,
			"aRange":[10,300],
		}
	}
}
