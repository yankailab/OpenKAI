{
	"name":"APP",
	"class":"Startup",
	"appName":"OpenKAI",
	"bLog":1,
	"bStdErr":1,
}

{
	"name":"console",
	"class":"_Console",
	"bON":1,
    {
		"name":"thread",
		"FPS":10,
	}
	"vBASE":["arduIO","PWMio","arduServo"],
}

{
	"name":"arduIO",
	"class":"_SerialPort",
	"bON":1,
	"bLog":0,
    {
		"name":"thread",
		"FPS":30,
	}
    {
		"name":"threadR",
		"FPS":30,
		"/vTwakeUp":["arduServo.threadR"],
	}
	"/port":"/dev/ttyUSB0",
	"port":"/dev/ttyACM0",
	"baud":115200,
}

{
	"name":"arduIO",
	"class":"_UDP",
	"bON":0,
	"bLog":1,
    {
		"name":"thread",
		"FPS":30,
	}
	"addr":"192.168.1.100",
	"port":8888,
	"_wakeUp":"arduServo",
}

{
	"name":"PWMio",
	"class":"_PWMio",
	"bON":0,
	"bLog":0,
    {
		"name":"thread",
		"FPS":30,
	}
    {
		"name":"threadR",
		"FPS":30,
	}
	"_IOBase":"arduIO",
	"nCr":5,
	"nCw":2,
	"vPWM":[1500,1500],
}

{
	"name":"arduServo",
	"class":"_ArduServo",
	"bON":1,
	"bLog":0,
    {
		"name":"thread",
		"FPS":30,
	}
	"_IOBase":"arduIO",
	{
		"name":"axis",
		{
			"name":"H",
			"pOrigin":1500.0,
			"pTarget":500.0,
			"pRange":[500,2500],
			"pErr":1,
		}
		{
			"name":"V",
			"pOrigin":1500.0,
			"pTarget":500.0,
			"pRange":[500,2500],
			"pErr":1,
		}
	}
}
